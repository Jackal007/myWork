<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>UserManage</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>admin-editUser.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <description>处理用户登录</description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.xmu.darksky.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description>用户信息查询</description>
    <display-name>CusServlet</display-name>
    <servlet-name>CusServlet</servlet-name>
    <servlet-class>com.xmu.darksky.servlet.CusServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CusServlet</servlet-name>
    <url-pattern>/CusServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description>店家信息管理</description>
    <display-name>SellerServlet</display-name>
    <servlet-name>SellerServlet</servlet-name>
    <servlet-class>com.xmu.darksky.servlet.SellerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SellerServlet</servlet-name>
    <url-pattern>/SellerServlet</url-pattern>
  </servlet-mapping>
  
</web-app>